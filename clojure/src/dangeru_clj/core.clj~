(ns dangeru-clj.core
  (:require [clj-http.client :as client]))

(defn testfn []
  (client/get "https://boards.dangeru.us/api.php?type=index&board=u&ln=5" {:insecure? true}))

(defn index [board, limit]
  (:body (client/get "https://boards.dangeru.us/api.php?" 
              {:query-params {:type "index" :board board :ln limit}
               :insecure? true
               :as :json})))

(println (index "u" 1))
